{
  "appId": "com.ubifind.app",
  "appName": "Ubifind",
  "webDir": "www",
  "server": {
    "cleartext": true,
    "allowNavigation": [
      "*.googleapis.com",
      "*.google.com",
      "*.gstatic.com",
      "*.ggpht.com"
    ]
  },
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 600,
      "androidSplashResourceName": "splash",
      "androidScaleType": "CENTER_CROP",
      "fadeInDuration": 300,
      "fadeOutDuration": 300,
      "backgroundColor": "#ffffff",
      "splashFullScreen": true,
      "splashImmersive": true
    },
    "CustomURLScheme": {
      "scheme": "ubifind",
      "host": "open"
    },
    
    "App": {
      "iosScheme": "ubifind",
      "androidScheme": "ubifind"
    },
    "GoogleAuth": {
      "scopes": ["profile", "email"],
      "serverClientId": "940873643414-856bc0no2p33f06lr9netrff6c77miul.apps.googleusercontent.com",
      "forceCodeForRefreshToken": true
    }
  },
  "android": {
    "allowMixedContent": true,
    "webContentsDebuggingEnabled": true
  }
}